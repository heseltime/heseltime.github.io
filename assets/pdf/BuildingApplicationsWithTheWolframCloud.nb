{RowBox[{"(","\[IndentingNewLine]",RowBox[{RowBox[{"data","=",RowBox[{"RandomChoice","[",RowBox[
{RowBox[{"{",RowBox[{"heads",",","tails"}],"}"}],",","#length"}],"]"}]}],";"," ","\[IndentingNewLine]",RowBox[
